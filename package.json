{
  "name": "transfermyti.me",
  "version": "1.3.0",
  "description": "Web app to transfer data between time tracking tools.",
  "repository": "https://github.com/mikerourke/transfermyti.me.git",
  "author": "Mike Rourke <mike.w.rourke@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "prebuild": "rimraf ./out/build",
    "build": "webpack --mode production --progress",
    "clockify:delete": "node scripts/run.mjs clockify-delete",
    "clockify:write": "node scripts/run.mjs clockify-write",
    "lint": "eslint --ext \".js,.mjs,.ts,.svelte\" \"{scripts,src}/**/*.{js,mjs,ts,svelte}\"",
    "prettier": "prettier --write \"{scripts,src,server}/**/*.{js,ts,css,html,svelte}\"",
    "start:web": "webpack serve --port 8080 --mode development",
    "start:server": "node scripts/run.mjs serve",
    "stylelint": "stylelint \"src/**/*.{css,html,svelte}\"",
    "svelte-check": "svelte-check",
    "test": "NODE_ENV=test node scripts/run.mjs tests --config jest.config.json",
    "typecheck": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "clsx": "^1.1.1",
    "cuid": "^2.1.8",
    "date-fns": "^2.28.0",
    "goober": "^2.1.9",
    "qs": "^6.10.3",
    "ramda": "^0.28.0",
    "redux": "^4.2.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.1.5",
    "typesafe-actions": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-typescript": "^7.16.7",
    "@testing-library/jest-dom": "^5.16.4",
    "@types/jest": "^27.5.1",
    "@types/jest-in-case": "^1.0.5",
    "@types/qs": "^6.9.7",
    "@types/ramda": "^0.28.12",
    "@types/redux-mock-store": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "babel-jest": "^28.1.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "body-parser": "^1.20.0",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.7.1",
    "debug": "^4.3.4",
    "dotenv": "^16.0.1",
    "eslint": "^8.15.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "express": "^4.18.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "jest-in-case": "^1.0.2",
    "mini-css-extract-plugin": "^2.6.0",
    "morgan": "^1.10.0",
    "node-fetch": "^3.2.4",
    "picocolors": "^1.0.0",
    "postcss-html": "^1.4.1",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "promise-throttle": "^1.1.2",
    "redux-mock-store": "^1.5.4",
    "redux-saga-test-plan": "^4.0.4",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.1",
    "stylelint": "^14.8.2",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-standard": "^25.0.0",
    "svelte": "^3.48.0",
    "svelte-check": "^2.7.0",
    "svelte-jester": "^2.3.2",
    "svelte-loader": "^3.1.2",
    "svelte-preprocess": "^4.10.6",
    "typescript": "^4.6.4",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0",
    "yargs": "^17.5.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "redux-saga-test-plan",
        "goober",
        "stylelint-a11y"
      ]
    }
  }
}
